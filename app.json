{
  "expo": {
    "name": "ParkEase",
    "slug": "parking",
    "version": "1.0.0",
    "scheme": "parking",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jyrwaboys.parking"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "be5b0d46-db1a-44ea-be98-6e44832080de"
      }
    },
    "owner": "jyrwaboys"
  }
}
